{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@locators/*": ["locators/*"],
      "@pages/*": ["pages/*"],
      "@tests/*": ["tests/*"],
      "@data/*": ["data/*"]
    }
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
